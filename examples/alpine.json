{
  "boot": {
    "efi": {
      "store": "build/efi_vars.fd"
    }
  },
  "identifier": "build/machine_id",
  "resources": {
      "cpus": 1,
      "memory": 256
  },
  "cmdline": "myargs",
  "serial": "none",
  "disks": [
    { 
      "mode": "block",
      "path": "build/apkovl.img"
    },
    { 
      "mode": "block",
      "path": "build/data.img",
      "readonly": false
    }
  ]
}
